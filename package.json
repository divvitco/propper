{
  "name": "@overlaylabs/propper",
  "version": "2.0.0",
  "description": "Transform React props on the fly without redux",
  "main": "/lib/index.js",
  "scripts": {
    "test": "jest && codecov",
    "build": "rm -rf lib/ && babel index.js -d lib/"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/overlay-labs/propper.git"
  },
  "keywords": [
    "react",
    "redux",
    "props",
    "transform"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/overlay-labs/propper/issues"
  },
  "homepage": "https://github.com/overlay-labs/propper#readme",
  "peerDependency": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-3": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "codecov": "^3.5.0",
    "coveralls": "^3.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "jest": "^24.8.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.9.0"
  }
}
